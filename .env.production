# Production Environment Variables for AWS Elastic Beanstalk
# These will be set through EB environment configuration

NODE_ENV=production
PORT=8080

# Database Configuration - Set this in EB Environment Variables
# DATABASE_URL=postgresql://username:password@host:port/database_name

# Security Settings
SESSION_SECRET=your-production-session-secret-here

# Application Settings
APP_NAME=MakanManager
APP_VERSION=1.0.0
DEFAULT_TASK_POINTS=50
SESSION_TTL_MINUTES=15

# Logging
LOG_LEVEL=info

# Features flags
ENABLE_DETAILED_LOGGING=false
ENABLE_METRICS=true